  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "yarn vitest run --config config/vitest.config.js --reporter verbose --passWithNoTests",
    "test:unit": "yarn vitest run --config config/vitest.unit.config.js --passWithNoTests",
    "test:int": "yarn vitest run --config config/vitest.int.config.js --passWithNoTests",
    "test:e2e": "yarn vitest run --config config/vitest.e2e.config.js --passWithNoTests",
    "lint": "eslint . --ext .js,.ts",
    "print": "yarn sls print",
    "offline": "yarn sls offline start -s dev -r ap-southeast-2",
    "offline:prod": "yarn sls offline start -s prod -r ap-southeast-1",
    "deploy": "yarn sls deploy -s dev -r ap-southeast-2",
    "deploy:dev": "yarn sls deploy -s dev -r ap-southeast-2",
    "deploy:prod": "yarn sls deploy -s prod -r ap-southeast-1",
    "package": "yarn sls package",
    "build": "yarn sls package",
    "format": "yarn prettier --write ./**/*.{js,ts}",
    "format:check": "yarn prettier --check ./**/*.{js,ts}"
  },
